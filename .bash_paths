# mess with $PATH #
###################

# default
PATH=/usr/bin:/bin:/usr/sbin:/sbin

# X11
# PATH=$PATH:/usr/X11/bin

# /usr/local
PATH=$PATH:/usr/local/bin:/usr/local/sbin

# homebrew
PATH=$PATH:/usr/local/homebrew/bin

# ~/bin
PATH=$PATH:$HOME/bin

# hadoop + hbase
PATH=$PATH:/usr/local/hadoop/bin
PATH=$PATH:/usr/local/hbase/bin

# mysql
PATH=$PATH:/usr/local/mysql/bin

# ec2 api+ami tools
export PATH=$PATH:$HOME/.ec2/tools/bin

# cloudera ec2
PATH=$PATH:/usr/local/cloudera-ec2

# tex
PATH=$PATH:/usr/local/texlive/2010/bin/x86_64-darwin

export PATH


# also, java's class path
export CLASSPATH=/usr/local/jars/*
