# amazon aws
if [ -f "$HOME/.amazon_keys" ]; then
  source "$HOME/.amazon_keys";
fi

# EC2
if [ -f "$HOME/.ec2rc" ]; then
  source "$HOME/.ec2rc";
fi

# bring in all keys.
ssh-add ~/.ssh/id_[rd]sa &> /dev/null

