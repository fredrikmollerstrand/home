# aliaser

# simple aliases
alias ll='ls -l'
alias lla='ls -la'

# scheme
alias mz='mzscheme -m'

# a little piece of the history
alias h='history | tail -n 32'

# textmate
function m() {
	mate $@ & 2> /dev/null
}

# subversion
# incidentally, this sequence of aliases mimic my workflow
alias sta='svn status'
alias sup='svn up'
alias sdi='svn diff'
alias sco='svn commit'

# view svn diff in textmate
alias sdim='svn diff | mate'


